{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "esnext",
      "module": "es2015",
      "jsx": "react",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
